{% extends "base.html.twig" %}
{% block body %}
<h1 class="mt-4 ml-5">Category</h1>
<div class="container">
    <div class="row">
        {% for category in categories %}
            <div class="container mt-5">
                <a href="{{ path("category_show", { categoryName : category.name }) }}">{{ category.name }}</a>
            </div>
        {% endfor %}
    </div>
</div>
{% endblock %}